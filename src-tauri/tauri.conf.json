{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "CC Version Guard",
  "version": "2.2.0",
  "identifier": "com.zendevve.ccversionguard",
  "build": {
    "frontendDist": "../src"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "CC Version Guard",
        "width": 520,
        "height": 580,
        "resizable": true,
        "minWidth": 400,
        "minHeight": 500,
        "minHeight": 500,
        "center": true,
        "decorations": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
